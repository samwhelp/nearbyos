pkgname=nearbyos-modeling-caja
pkgver=0.1
pkgrel=1
pkgdesc="Nearbyos / Modeling / Caja"
url="https://github.com/samwhelp/nearbyos/tree/main/project/nearbyos-system/nearbyos-packaging/pack/core/tool/nearbyos-modeling-caja"
arch=('any')
license=('MIT')
makedepends=()
depends=()
conflicts=()
provides=("${pkgname}")
options=(!strip !emptydirs)


prepare () {

	cp -af ../asset/. "${srcdir}"

}


package () {

	##
	## Main Config
	##

	cp -af "${srcdir}/overlay/." "${pkgdir}"

}
